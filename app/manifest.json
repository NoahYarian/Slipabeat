{
  	"name": "__MSG_appName__",
  	"description": "__MSG_appDescription__",
  	"version": "1",
  	"manifest_version": 2,
  	"default_locale": "en",
  	"permissions": [
  		"serial",
  		"usb",
      {
        "usbDevices": [
          {
            "vendorId": 7992,
            "productId": 30
          }
        ]
      },
  		"<all_urls>"
  	],
  	"icons": {
  		"16": "assets/images/icon-16.png",
  		"128": "assets/images/icon-128.png"
  	},
  	"app": {
  		"background": {
  			"scripts": [
  				"scripts/main.js",
  				"scripts/chromereload.js"
  			]
  		}
  	}
  }
